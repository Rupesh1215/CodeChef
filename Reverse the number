import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		
		while(t-- > 0){
		    int n = s.nextInt();
		    int digit = 0;
		    int reversed = 0;
		    
		    while(n>0){
		         digit = n%10;
		         reversed = reversed *10 + digit;
		         n = n/10;
		    }
		    
		    System.out.println(reversed);
		}

	}
}
