class Solution {
    public String checkCoupon(int n, int x, int y, int[] prices) {
        // write your code here 
        
        int totalCost = 0;
        int dtotalCost = 0;
        
        for(int i = 0;i < n;i++){
            totalCost += prices[i];
            dtotalCost += Math.max(0, prices[i] - y);
        }
        
        if(totalCost > dtotalCost+x){
            return "COUPON";
        }
        else{
            return "NO COUPON";
        }
    }
}
